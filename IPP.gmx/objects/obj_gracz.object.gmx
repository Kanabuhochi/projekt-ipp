            <string>///Ruch

key_right = keyboard_check(ord("D"));
key_left = -keyboard_check(ord("A"));
key_jump = keyboard_check_pressed(ord("W"));

move = key_left + key_right;
h_spd = move * move_spd;
if (v_spd &lt; 10) v_spd += graw;

if (place_meeting(x,y+1,obj_ziemia))
{
    v_spd = key_jump * -jump_spd;
}

if (place_meeting(x,y-2,obj_ziemia))
{
    v_spd = 1;
}



if (key_jump) &amp;&amp; (place_meeting(x+1,y,obj_ziemia) || place_meeting(x-1,y,obj_ziemia))
{
    v_spd = -jump_spd;
}

//Kolizja

if(place_meeting(x+h_spd,y,obj_ziemia))
{
    while(!place_meeting(x+sign(h_spd),y,obj_ziemia))
    {
        x += sign(h_spd);
    }
    h_spd = 0;
}
x += h_spd;

if(place_meeting(x,y+v_spd,obj_ziemia))
{
    while(!place_meeting(x,y+sign(v_spd),obj_ziemia))
    {
        y += sign(v_spd);
    }
    v_spd = 0;
}

if(place_meeting(x,y+v_spd,obj_platforma))
{
    while(!place_meeting(x,y+sign(v_spd),obj_platforma))
    {
        y += sign(v_spd);
    }
    v_spd = 0;
}

y += v_spd;
if (move!=0) image_xscale = move;
if (place_meeting(x,y+1,obj_ziemia))
{
    if(move!=0) sprite_index = spr_gracz_run;
    else sprite_index = spr_gracz_idle;
}
else
{
    if (v_spd &lt; 0) sprite_index = spr_gracz_jump;
    else sprite_index = spr_gracz_fall;
}
</string>
